// 1. Define the ring
ring R = 0, (x0, x1), dp;  // Polynomial ring Q[x0, x1]
// 2. Define the matrices A: M2 b M1 and B: M1 b M0
// Example: matrix A with 2 columns, matrix B with 2 rows
matrix A[3][2] = x0, x1,
                 0, x0,
                 0, 0;
matrix B[2][3] = x0, 0, 0,
                 x1, x0, x1;
// 3. Create modules for the maps (columns are generators of image)
module imA = A;        // image of A b
 M1
module kerB = kernel(B); // kernel of B b
 M1
// 4. Compute homology: H = ker(B)/im(A)
module H = kerB / imA;
// 5. Print the result
"H = ker(B)/im(A):";
print(H);
;
;
;
;
q
quit
